{
    "name": "laravel/ai",
    "description": "The official AI SDK for Laravel.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ai"
    ],
    "homepage": "https://github.com/laravel/ai",
    "support": {
        "issues": "https://github.com/laravel/ai/issues",
        "source": "https://github.com/laravel/ai"
    },
    "require": {
        "php": "^8.4",
        "illuminate/console": "^12.0",
        "illuminate/container": "^12.0",
        "illuminate/contracts": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/json-schema": "^12.0",
        "illuminate/support": "^12.0",
        "laravel/prompts": "^0.3.6",
        "laravel/serializable-closure": "^2.0",
        "prism-php/prism": "^0.99.0"
    },
    "require-dev": {
        "laravel/pint": "^1.26",
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^10.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Laravel\\Ai\\": "src/"
        },
        "files": [
            "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Ai\\AiServiceProvider"
            ]
        }
    }
}
